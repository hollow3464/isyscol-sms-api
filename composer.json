{
    "name": "hollow3464/isyscol-sms-api",
    "description": "SMS Api Helper",
    "type": "library",
    "version": "2.0.0",
    "autoload": {
        "psr-4": {
            "Hollow3464\\SmsApiHelper\\": "src/"
        }
    },
    "authors": [
        {
            "name": "David Chamorro",
            "email": "daved3464@gmail.com"
        }
    ],
    "scripts": {
        "fmt": "vendor/bin/pint",
        "lint": "vendor/bin/phpstan",
        "test": "vendor/bin/pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "require": {
        "cuyz/valinor": "^1.13",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0 || ^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/pint": "^1.15",
        "pestphp/pest": "^2.34",
        "phpstan/phpstan": "^1.10",
        "vlucas/phpdotenv": "^5.5"
    }
}
